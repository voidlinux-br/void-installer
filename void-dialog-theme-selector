# Exibir o menu com números e nomes de temas
theme_list=()
count=1
for key in "${!themes[@]}"; do
	theme_list+=("$key" "$count")
	((count++))
done

theme_choice=$(dialog \
              --colors \
              --title "$ctitle" \
              --backtitle "$ccabec" \
              --menu "Escolha o tema desejado" 0 60 20 "${theme_list[@]}" 2>&1 >/dev/tty)

# Salvar o tema escolhido no arquivo .dialogrc
if [ -n "$theme_choice" ]; then
	echo "${themes[$theme_choice]}" >"$HOME/.dialogrc"
	dialog --msgbox "Tema '$theme_choice' aplicado com sucesso!" 0 0
else
	dialog --msgbox "Nenhum tema escolhido!" 0 0
fi
