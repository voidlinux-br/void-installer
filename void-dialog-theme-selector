themes["Basic Cyan"]="screen_color = (white,black,off)|dialog_color = (white,black,off)|title_color = (cyan,black,on)|button_active_color = (black,cyan,on)|check_selected_color = button_active_color|use_colors = on"

#dom 11 jan 2026 09:51:57 -04
# Converte o array associativo "themes" do formato antigo
# (com valores contendo quebras de linha) para o novo formato,
# onde cada campo é separado por "|" em uma única linha.
convert_old_format_array_theme() {
  # Loop por todas as chaves do array associativo "themes"
  for k in "${!themes[@]}"; do
    # Obtém o valor bruto do tema referente a essa chave
    val=${themes[$k]}

    # Substitui quebras de linha ($'\n') por pipes "|"
    # Obs: $'\n' é escape literal de newline no Bash, não texto "\n"
    val=${val//$'\n'/|}

    # Remove pipe inicial caso exista (resultado comum quando a
    # primeira linha do valor era precedida de newline)
    val=${val#|}

    # Imprime o tema já convertido no formato:
    # themes["Nome"]="campo1|campo2|campo3|..."
    # Ideal para importar no novo sistema de temas
    printf 'themes["%s"]="%s"\n' "$k" "$val"
  done
}

apply_theme() {
	local theme_name="$1"
	local theme_data="${themes[$theme_name]}"
	[[ -z "$theme_data" ]] && return 1

	# explode por |
	IFS='|' read -r -a fields <<<"$theme_data"

	# escreve literal no .dialogrc
	: >"$HOME/.dialogrc" # limpa antes
	for field in "${fields[@]}"; do
		# remove espaços extras do começo/fim
		field="${field#"${field%%[![:space:]]*}"}"
		field="${field%"${field##*[![:space:]]}"}"
		echo "$field" >>"$HOME/.dialogrc"
	done
}

sh_main() {
	# Preparar lista ordenada
	theme_list=()
	mapfile -t sorted_keys < <(printf "%s\n" "${!themes[@]}" | sort)

	# Contador para a numeração na coluna 2
	local count=1
	for key in "${sorted_keys[@]}"; do
		theme_list+=("$key" "$count")
		((count++))
	done

	while true; do
		theme_choice=$(${DIALOG} \
			--default-item "$theme_choice" \
			--colors \
			--title "$ctitle" \
			--backtitle "$ccabec" \
			--ok-label "'$(gettext 'Selecionar')" \
			--cancel-label "'$(gettext 'Sair')" \
			--menu "${cmsg_LABEL}\n\n${YELLOW}$(gettext "Escolha o tema desejado"):${RESET}" \
			22 0 2 "${theme_list[@]}" \
			2>&1 >/dev/tty)

		if [ -n "$theme_choice" ]; then
			apply_theme "$theme_choice"
			${DIALOG} --msgbox "$(gettext "Tema '$theme_choice' aplicado com sucesso!")" 0 0
		else
			exit 0
		fi
	done
}

sh_setVarColors
sh_main
